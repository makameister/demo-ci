{
    "name": "maka/demo-ci",
    "description": "Projet de demo pour la mise en place du chaine d'integration continue",
    "autoload":{
        "psr-4": {
            "Framework\\": "src/Framework",
            "App\\": "src/App"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "Test\\": "test"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {},
    "require-dev": {
        "filp/whoops": "^2.7",
        "phpunit/phpunit": "^9.1",
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/php-timer": "^5.0.3",
        "spatie/phpunit-watcher": "^1.22",
        "phpstan/phpstan": "^0.12.52",
        "squizlabs/php_codesniffer": "^3.5",
        "mockery/mockery": "^1.4",
        "phpspec/prophecy-phpunit": "^2.0",
        "phploc/phploc": "^7.0",
        "sebastian/phpcpd": "^6.0",
        "pdepend/pdepend": "^2.8",
        "phpmd/phpmd": "^2.9"
    }
}
